 <div class="wrapper">
            
            	
            	
            	 <h1 data-localize="modules.contact.account-responsible">Your account responsible</h1>
            	<div class="contact-info">
                               
                                <div class="profile-pic pull-left"><a class="contact-details" user="<%= salesData.salesPerson.id %>"><img style="width:53px;" src="<%= salesData.salesPerson.avatar %>" onerror="setDefaultMainAvatar(this)"></a></div>
                                <div class="details">
                                    <a class="name contact-details" user="<%= salesData.salesPerson.id %>"><%= salesData.salesPerson.name %></a>
                                    <div class="job-title"><%= salesData.salesPerson.position %></div>
                                    <div class="actions">
                                        <a class="btn btn-mini quick-message" user="<%= salesData.salesPerson.id %>"><i class="icon-quick-message"><%== $.View("templates/layouts/icons/contact_icon.ejs",{}) %></i> </a>
                                    </div>
                                </div>
                            </div>
                           
                            <h1 data-localize="modules.contact.pm-responsible">Your project manager responsible</h1>
                            <div class="contact-info <% if(salesData.additionalResponsibles==null || salesData.additionalResponsibles.length==0) { %> last<% } %> ">
                                
                                <div class="profile-pic pull-left"><a class="contact-details" user="<%= salesData.pmResponsible.id %>"><img style="width:53px;" src="<%= salesData.pmResponsible.avatar %>" onerror="setDefaultMainAvatar(this)"></a></div>
                                <div class="details">
                                    
                                    <a class="name contact-details" user="<%= salesData.pmResponsible.id %>"><%= salesData.pmResponsible.name %></a>
                                    <div class="job-title"><%= salesData.pmResponsible.position %></div>

                                    <div class="actions">
                                        <a class="btn btn-mini quick-message"  user="<%= salesData.pmResponsible.id %>" ><i class="icon-quick-message"><%== $.View("templates/layouts/icons/contact_icon.ejs",{}) %></i> </a>
                                    </div>
                                </div>
                            </div>
                            
                            
                              <% if(salesData.additionalResponsibles!=null && salesData.additionalResponsibles.length>0) { %>
                              <h1 data-localize="modules.contact.additional-responsible">Additional Responsible</h1>
                                
                                
                             <% for(var i=0; i< salesData.additionalResponsibles.length; i++){ %>
                       
                              
                              
                               <div class="contact-info <% if(salesData.additionalResponsibles==null || salesData.additionalResponsibles.length==0) { %> last<% } %> ">
                                
                                <div class="profile-pic pull-left"><a class="contact-details" user="<%= salesData.additionalResponsibles[i].id %>"><img style="width:53px;" src="<%= salesData.additionalResponsibles[i].avatar %>" onerror="setDefaultMainAvatar(this)"></a></div>
                                <div class="details">
                                    <a class="name contact-details" user="<%= salesData.additionalResponsibles[i].id %>"><%= salesData.additionalResponsibles[i].name %></a>
                                    <div class="job-title"><%= salesData.additionalResponsibles[i].position %></div>
                                    <div class="actions">
                                        <a class="btn btn-mini quick-message"  user="<%= salesData.additionalResponsibles[i].id %>" ><i class="icon-quick-message"><%== $.View("templates/layouts/icons/contact_icon.ejs",{}) %></i> </a>
                                    </div>
                                </div>
                            </div>
                              
                             <% } %>
                           
                             <% } %>
            	

            	
            	
             
            	
            </div>
            <div class="bottom-mask"></div>