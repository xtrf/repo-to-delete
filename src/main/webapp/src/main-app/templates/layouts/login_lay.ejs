<div id="all-content">

	<header id="language-bar" class="embossed-border-bottom">
		<div class="container">
			<div class="language-choice dropdown pull-right span3">
				<div class="dropdown-toggle" id="language-choosen" data-toggle="dropdown">
					<span class="flag-en"></span> English <b class="caret"></b></div>


				<ul class="dropdown-menu">
					<% $.each(locales,function(i,locale){ %>
					<li><a language="<%= locale.id %>" href="javascript:void(0);">
						<span class="flag-<%= locale.id %>"></span> <%= locale.name %></a></li>
					<% }) %>


				</ul>
			</div>
		</div>

	</header>
	<div class="container row" id="form-container">

	</div>


</div>