<div data-status="empty" class="module module-widget-new module-quotes">
                <header>
                  <span><a <% if(permissionsTable['quotes_browse']==1){ %> href="#!quotes/active/1"  <% } %>  data-localize="<%= localizeHeader %>">Quotes statistics</a></span>
                  <span class="tools">
                   <% if(dashboards.options.IE8==false){ %> <a class="widget-detach"><i class="icon-widget-detach"><%== $.View("templates/layouts/icons/x_icon.ejs",{}) %></i></a> <% } %>
                  </span>
                </header>
                <section class="content">
                      <div class="empty-set-placeholder">
                          <div class="active-counter">
                            <p data-localize="general.you-have-0-active-quotes">You have {0} active quotes</p>
                          </div>
                           <a class="btn btn-ncta btn-large raq" data-localize="general.raq">Request a quote</a>
                      </div>   
                </section>
              </div>