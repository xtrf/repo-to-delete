<div data-status="active" class="module module-widget-new module-quotes">
                <header>
                  <span><a <% if(permissionsTable['quotes_browse']==1){ %> href="#!quotes/active/1"  <% } %> class="module-header-text" data-localize="<%= localizeHeader %>">Recent quotes</a></span>
                  <span class="tools">
                    <% if(dashboards.options.IE8==false){ %> <a class="widget-detach"><i class="icon-widget-detach"><%== $.View("templates/layouts/icons/x_icon.ejs",{}) %></i></a> <% } %>
                  </span>
                </header>
                <section class="content">
                <ul class="entity-list">
                <% $.each(this.quotes, function(i, quote){ %>
                        
                                           
                           <li <% if(quote.status=="SENT") { %> class="quote-wfa" <% } else { %> class="quote-p" <% } %>>
                           <span class="name">
                           <%== $.View("templates/includes/quote_name.ejs", {quote:quote}) %>
		                   </span>
		                    <span class="info">
		                      <big><%= quote.totalAgreed.formattedAmount %></big>
		                     <% if(quote.status=="SENT") { %>  <a class="btn btn-mini btn-ncta btn-accept-quote-dashboard" data-quote-id="<%= quote.id %>" data-localize="modules.quotes.accept">Accept</a><% } %>
		                     
		                     <% if(permissionsTable['quote_view']==1){ %>
		                      <a rel="tooltip-dash" class="preview-link" href="#!quote/<%= quote.id %>" title="View quote" data-localize-2="modules.quotes.view-quote"><i class="icon-preview"></i></a>
		                     <% } %>
		                    </span>
		                  </li>
		                          
                          
                          
                          <% }) %>
                
                
                
              
                </ul>
                </section>
              </div>