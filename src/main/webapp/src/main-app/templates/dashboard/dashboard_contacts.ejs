
                <header>
                  <h2 data-localize="modules.contact.header">Contact</h2>
                  <span class="tools">
                    <a id="instant-contact" class="btn btn-mini" href="javascript:void(0);"><i class="icon-instant-contact"></i> <span data-localize="modules.contact.contact-button">Contact</span></a>
                  </span>
                </header>
                <section class="content">
                  <div class="contact-info">
                    <h3 data-localize="modules.contact.account-responsible">Your account responsible</h3>
                    <div class="profile-pic pull-left"><a class="name contact-details" user="<%= salesPerson.id %>"><img src="<%= salesPerson.avatar %>" onerror="setDefaultAvatar2(this)"></a></div>
                    <div class="details">
                       <a class="name contact-details" user="<%= salesPerson.id %>"><%= salesPerson.name %></a>
                      <div class="job-title"><%= salesPerson.position %></div>
                      <div class="actions">
                        <a class="btn btn-mini quick-message" user="<%= salesPerson.id %>"><i class="icon-quick-message"></i> <span data-localize="modules.contact.send-message">Send message</span></a>
                      </div>
                    </div>
                  </div>
                  <div class="contact-info last">
                    <h3 data-localize="modules.contact.pm-responsible">Your project manager responsible</h3>
                    <div class="profile-pic pull-left"><a class="name contact-details" user="<%= pmResponsible.id %>"><img src="<%= pmResponsible.avatar %>" onerror="setDefaultAvatar2(this)"></a></div>
                    <div class="details">
                     <a class="name contact-details" user="<%= pmResponsible.id %>"><%= pmResponsible.name %></a>
                      <div class="job-title"><%= pmResponsible.position %></div>
                      <div class="actions">
                        <a class="btn btn-mini quick-message"  user="<%= pmResponsible.id %>"><i class="icon-quick-message"></i> <span data-localize="modules.contact.send-message">Send message</span></a>
                      </div>
                    </div>
                  </div>
                </section>
         